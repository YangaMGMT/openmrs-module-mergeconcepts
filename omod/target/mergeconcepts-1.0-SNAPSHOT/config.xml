<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>mergeconcepts</id>
	<name>Merge Concepts Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.mergeconcepts</package>
	<author>jordankellerstrass</author>
	<description>
		MeIUpdates concept id references
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/mergeconcepts/update.rdf</updateURL>
	<require_version>1.8.0</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.mergeconcepts.MergeConceptsActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.mergeconcepts.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		MergeConcepts.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with mergeconcepts. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

